<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="com.wsdy.saasops.modules.operate.dao.OprHelpContentMapper">


	<update id="updatContentAvailable" parameterType="com.wsdy.saasops.modules.operate.entity.OprHelpContent">
		update help_content set available=#{available} where id=#{id}
	</update>
	<update id="deleteContent" parameterType="com.wsdy.saasops.modules.operate.entity.OprHelpContent">
		update help_content set isdelete=#{isdelete} where id=#{id}
	</update>


</mapper>